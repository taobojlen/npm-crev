{
  "name": "crev",
  "description": "Code REView for npm",
  "version": "0.0.0",
  "author": "Tao BojlÃ©n",
  "bin": {
    "crev": "bin/run"
  },
  "bugs": "https://github.com/taobojlen/npm-crev/issues",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/taobojlen/npm-crev",
  "keywords": [
    "oclif"
  ],
  "license": "AGPL-3.0-or-later",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "crev",
    "topics": {
      "id": {
        "description": "commands to interact with crev IDs"
      }
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "taobojlen/npm-crev",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "npm run lint -- --fix",
    "pretty": "prettier --write .",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "pretest": "npm run lint",
    "test": "nyc --reporter=lcov --reporter=text --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --timeout 10000",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "argon2": "^0.27.2",
    "chai-bytes": "^0.1.2",
    "cli-ux": "^5.5.1",
    "enquirer": "^2.3.6",
    "env-paths": "^2.2.1",
    "graphology": "^0.19.2",
    "graphology-simple-path": "^0.1.0",
    "gunzip-maybe": "^1.4.2",
    "hosted-git-info": "^4.0.2",
    "humps": "^2.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "miscreant": "^0.3.2",
    "node-fetch": "^2.6.1",
    "sodium-native": "^3.2.0",
    "tar-fs": "^2.1.1",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.1",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/hosted-git-info": "^3.0.1",
    "@types/humps": "^2.0.0",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.2",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "@types/sinon": "^10.0.0",
    "@types/sodium-native": "^2.3.5",
    "@types/tar-fs": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^11.0.3",
    "graphology-types": "^0.19.2",
    "mocha": "^8.4.0",
    "mock-fs": "^4.14.0",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "sinon": "^10.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.4"
  }
}
